{
  "expo": {
    "name": "zootracker",
    "slug": "zootracker",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Permita $(PRODUCT_NAME) acessar suas fotos.",
          "savePhotosPermission": "Permita $(PRODUCT_NAME) salvar suas fotos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  },
  "name": "zootracker"
}
